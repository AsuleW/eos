{
   "name": "eos",
   "version-string": "2.1.0",
   "port-version": 1,
   "dependencies": [
		"openssl",
      "gmp",
      "libpqxx",
      "amqpcpp",
      "rocksdb",
      "yubihsm"
   ],
   "$default-features-explanation": "Users using this library transitively will get all dependencies from vcpkg automatically",
   "default-features": [
      "vcpkg-boost",
      "vcpkg-llvm"
   ],
   "features": {
      "vcpkg-boost": {
         "description": "boost library from vcpkg",
         "dependencies": [
            "boost-date-time",
            "boost-test",
            "boost-asio",
            "boost-filesystem",
            "boost-program-options",
            "boost-iostreams",
            "boost-interprocess",
            "boost-beast",
            "boost-multi-index",
            "boost-multiprecision",
            "boost-signals2",
            "boost-accumulators",
            "boost-hana",
            "boost-assign",
            "boost-convert",
            "boost-process",
            "boost-bimap",
            "boost-dll",
            "boost-lockfree"
         ]
      },
      "vcpkg-llvm": {
         "description": "LLVM from vcpkg",
         "dependencies": [
            {
               "name": "llvm",
               "features": [
                  "enable-rtti"
               ],
               "platform": "linux"
            }
         ]
      }
   }
}